{
  "profiles": {
    "Discount.API": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger ",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5003"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTP_PORTS": "8003"
      },
      "publishAllPorts": true,
      "useSSL": false
    }
  }
}
